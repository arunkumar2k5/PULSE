# OpenRouter API Configuration
VITE_OPENROUTER_API_KEY=your_openrouter_api_key_here

# OpenRouter API Base URL
VITE_OPENROUTER_BASE_URL=https://openrouter.ai/api/v1

# Default LLM Model (Claude 3.5 Sonnet)
VITE_LLM_MODEL=anthropic/claude-3.5-sonnet

# Optional: Your site URL (for OpenRouter rankings)
VITE_SITE_URL=http://localhost:5173

# Optional: Your site name (for OpenRouter rankings)
VITE_SITE_NAME=PULSE - PlantUML Script Engine

# PlantUML Server Configuration
# For development: use public server
VITE_PLANTUML_SERVER_URL=https://www.plantuml.com/plantuml

# For production: use local Docker server
# VITE_PLANTUML_SERVER_URL=http://localhost:8080
